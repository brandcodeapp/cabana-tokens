    "footer": {
      "container": {
        "value": {
          "fill": "{section.footer.container-background-color}",
          "horizontalPadding": "{section.footer.padding-x}",
          "verticalPadding": "{section.footer.padding-y}"
        },
        "type": "composition"
      },
      "container-overlay": {
        "value": "{color.transparent}",
        "type": "color",
        "description": "The section container has a separate background and overlay. You could use the overlay to add a gradient or opacity layer on top of your background color or image fill."
      },
      "container-background-color": {
        "value": "{semantic.color.accent.emphasis}",
        "type": "color",
        "description": "Every section container can have a unique background color or image. On top of the background a gradient or opactive layer can be applied. Use the container-overlay token to achieve this."
      },
      "container-bg-image": {
        "value": "null",
        "type": "asset",
        "description": "Add an image address pointing to a public url"
      },
      "divider-color": {
        "value": "{semantic.color.accent.default}",
        "type": "color"
      },
      "body-color": {
        "value": "{semantic.color.fg.on-accent}",
        "type": "color"
      },
      "logo-color": {
        "value": "{semantic.color.fg.on-accent}",
        "type": "color"
      },
      "padding-x": {
        "value": "{semantic.space-around.section.lg}",
        "type": "spacing"
      },
      "padding-y": {
        "value": "{semantic.space-around.section.xs}",
        "type": "spacing"
      },
      "space-between": {
