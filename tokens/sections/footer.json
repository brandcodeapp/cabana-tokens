        "icon-color": {
          "value": "{semantic.color.bg.surface}",
          "type": "color"
        },
        "icon-size": {
          "value": "{semantic.size.section.md}",
          "type": "sizing"
        },
        "space-between": {
          "value": "{semantic.space-between.comp.sm}",
          "type": "spacing"
        }
      }
